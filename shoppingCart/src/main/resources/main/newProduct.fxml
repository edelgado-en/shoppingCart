<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="33.0" layoutY="22.0" prefHeight="17.0" prefWidth="139.0" text="Add New Product">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="140.0" layoutY="89.0" prefHeight="17.0" prefWidth="47.0" text="Name" />
      <TextField fx:id="productName" layoutX="195.0" layoutY="85.0" />
      <Label layoutX="140.0" layoutY="134.0" prefHeight="17.0" prefWidth="39.0" text="Price" />
      <TextField fx:id="productPrice" layoutX="195.0" layoutY="130.0" prefHeight="25.0" prefWidth="47.0" />
      <Label layoutX="140.0" layoutY="192.0" text="Quantity" />
      <TextField fx:id="productQuantity" layoutX="195.0" layoutY="188.0" prefHeight="25.0" prefWidth="47.0" />
      <Button layoutX="239.0" layoutY="251.0" mnemonicParsing="false" onAction="#addProduct" prefHeight="25.0" prefWidth="123.0" text="Add Product" />
      <Button layoutX="172.0" layoutY="251.0" mnemonicParsing="false" onAction="#backToInventory" text="Back" />
      <Label fx:id="responseMessage" layoutX="195.0" layoutY="52.0" prefHeight="17.0" prefWidth="161.0" />
   </children>
</AnchorPane>
