      <TextField fx:id="password" layoutX="230.0" layoutY="192.0" />
      <Button layoutX="230.0" layoutY="250.0" mnemonicParsing="false" onAction="#signUp" prefHeight="26.0" prefWidth="160.0" text="Sign up" />
